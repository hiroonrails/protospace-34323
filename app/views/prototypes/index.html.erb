<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        <%= link_to "こんにちは、#{current_user.name}さん", user_path(current_user) %>
      </div>
    <% else %>
      <%= link_to "登録せえへんのか？？", new_user_registration_path, class: :greeting__link%>
    <%end%>
    <div class="card__wrapper">
      <%= render partial: "prototype", collection: @prototypes %>
    </div>
  </div>
</main>